{
    "manifest_version": 3,
    "name": "Pruffme Tweaker",
    "description": "Fix various Pruffme bugs",
    "version": "1.1",
    "author": "Arseniy Virachev aka Trigger239",
    "action": {
        "default_title": "Fix bugs!"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
}